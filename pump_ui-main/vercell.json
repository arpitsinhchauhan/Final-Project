{
    "version": 2,
    "builds": [
        {
            "src": "angular.json",
            "use": "@vercel/angular",
            "config": {
                "installCommand": "npm install --legacy-peer-deps"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
